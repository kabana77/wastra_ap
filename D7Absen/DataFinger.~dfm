    SQL.Strings = (
      
        'SELECT p.no_id, q.kary_negara as nik, p.tanggal, p.jam, q.kary_n' +
        'ama as nama_karyawan from (select a.trans_kode as no_id, a.trans' +
        '_tgl as tanggal , a.trans_jam as jam FROM  absensi a'
      'where a.trans_tgl>='#39'2010-05-04'#39' and a.trans_tgl<'#39'2010-05-05'#39') p'
      'left outer join karyawan q on (p.no_id=q.kary_kode)')
