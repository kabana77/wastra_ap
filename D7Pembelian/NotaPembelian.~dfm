      ' select'
      '  d.ppn,'
      '  d.ispjk,'
      '  d.cara_bayar,'
      '  trunc(d.tanggal) as tgl_po,'
      '  d.um,'
      '  d.diskon,'
      '  d.by_lain2,'
      '  d.ket_lain2'
      ' from '
      ' (select a.no_nota, b.no_reg_po'
